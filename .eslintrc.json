{
  "env": {
    "node": true
    },
  "extends": "airbnb",
  "rules": {
    "padded-blocks": [2, "always"],
    "no-multi-spaces": 0,
    "no-var": 0,
    "consistent-return": 0,
    "brace-style": [2, "stroustrup"],
    "no-console": 0,
    "func-names": 0,
    "max-len": 0,
    "comma-dangle": 0,
    "new-cap": 0,
    "no-param-reassign": 0
  }
}
